{
  "name": "@webok/api",
  "version": "0.0.0",
  "repository": "https://github.com/webokio/webok/tree/master/packages/api",
  "main": "lib/main.js",
  "scripts": {
    "clean": "webok-dev clean",
    "build": "webok-dev build",
    "format": "webok-dev format migrations/*.ts",
    "postformat": "npm run tslint",
    "tslint": "webok-dev tslint",
    "publish:prepare": "npm run clean && npm run build",
    "dev": "cross-env NODE_ENV=development NODE_CONFIG_DIR=./config node lib/main",
    "dev:generate": "cross-env NODE_ENV=development NODE_CONFIG_DIR=./config typeorm migration:generate -n",
    "dev:migrate": "cross-env NODE_ENV=development NODE_CONFIG_DIR=./config typeorm migration:run",
    "dev:rollback": "cross-env NODE_ENV=development NODE_CONFIG_DIR=./config typeorm migration:revert"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.3.0",
    "@webok/pages": "^0.0.0",
    "class-transformer": "^0.2.0",
    "config": "^3.0.1",
    "pg": "^7.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "typeorm": "^0.2.14"
  },
  "devDependencies": {
    "@webok/dev": "^0.0.0",
    "cross-env": "^5.2.0"
  }
}
