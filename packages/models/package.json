{
  "name": "@webok/models",
  "version": "0.0.0",
  "repository": "https://github.com/webokio/webok/tree/master/packages/models",
  "main": "lib/index.js",
  "scripts": {
    "clean": "webok-dev clean",
    "build": "webok-dev build",
    "format": "webok-dev format migrations/*",
    "postformat": "npm run tslint",
    "tslint": "webok-dev tslint",
    "publish:prepare": "npm run clean && npm run build",
    "dev:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -f src/ormconfig -n",
    "dev:migrate": "ts-node ./node_modules/.bin/typeorm migration:run -f src/ormconfig",
    "dev:rollback": "ts-node ./node_modules/.bin/typeorm migration:revert -f src/ormconfig"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "pg": "^7.8.1",
    "typeorm": "^0.2.14",
    "typescript-optional": "^2.0.1"
  },
  "devDependencies": {
    "@webok/dev": "^0.0.0",
    "ts-node": "^8.0.2"
  }
}
