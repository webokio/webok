{
  "name": "@webok/dev",
  "description": "Common development scripts and config files for WebOK",
  "version": "0.0.0",
  "repository": "https://github.com/webokio/webok/tree/master/tools/dev",
  "bin": {
    "webok-dev": "./lib/cli.js"
  },
  "scripts": {
    "clean:all": "node ./lib/cli clean",
