    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "lerna": "^3.13.1",
    "npm-which": "^3.0.1",
    "package-json": "^6.0.0",
    "prettier": "^1.16.4",
    "progress": "^2.0.3",
    "rimraf": "^2.6.3",
    "table": "^5.2.3",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/config": "^0.0.34",
    "@types/cross-spawn": "^6.0.0",
    "@types/node": "^11.10.5",
